doctype html
html(lang='en')
  head
    block head
        title Beez Window Detailing
        //- link(rel="shortcut icon", href="/public/favicon.png")

      block fonts
        link(href='http://fonts.googleapis.com/css?family=Questrial|Raleway:600|Comfortaa:700|Open+Sans+Condensed:300' rel='stylesheet' type='text/css')

      block stylesheets
        != css("normalize")
        != css("foundation.min.css")

        != css("header")
        != css("index")
        link(href="//netdna.bootstrapcdn.com/font-awesome/4.1.0/css/font-awesome.min.css" rel="stylesheet")

      block scripts
        script(src="//ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js")
        != js("modernizr.js")
        != js("fastclick.js")
        != js("foundation/foundation.min.js")
        != js("foundation/foundation.topbar.js")
        != js("jquery.color.min.js")
        != js("ga-tracking.js")

  body
    include header
    include index
